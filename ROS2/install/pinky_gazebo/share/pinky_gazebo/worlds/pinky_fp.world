  <model name="camera">
    <static>true</static>
      <pose>0 0 1.6 0 1.57 1.57</pose>
      <link name="link">
        <pose>0.05 0.05 0.05 0 0 0</pose>
        <inertial>
          <mass>0.1</mass>
          <inertia>
            <ixx>0.000166667</ixx>
            <iyy>0.000166667</iyy>
            <izz>0.000166667</izz>
          </inertia>
        </inertial>
        <collision name="collision">
          <geometry>
            <box>
              <size>0.1 0.1 0.1</size>
            </box>
          </geometry>
        </collision>
        <sensor name="camera" type="camera">
          <camera>
            <horizontal_fov>2.094</horizontal_fov>
            <image>
              <width>640</width>
              <height>480</height>
            </image>
            <clip>
              <near>0.1</near>
              <far>100</far>
            </clip>
          </camera>
          <always_on>1</always_on>
          <update_rate>30</update_rate>
          <visualize>true</visualize>
          <topic>camera</topic>
        </sensor>
      </link>
  </model>

  <model name="team2_map">
    <link name="link">
      <inertial>
        <mass>1</mass>
        <inertia>
          <ixx>0.0</ixx>
          <ixy>0.0</ixy>
          <ixz>0.0</ixz>
          <iyy>0.0</iyy>
          <iyz>0.0</iyz>
          <izz>0.0</izz>
        </inertia>
      </inertial>
      <collision name="collision">
        <pose>-1.125 1.715 0 0 0 0</pose>
        <geometry>
          <mesh>
            <uri>model://team2_map/map.stl</uri>
            <!-- <scale>0.8825 0.8825 1</scale> -->
          </mesh>
        </geometry>
      </collision>
      <visual name="visual">
        <pose>-1.125 1.715 0 0 0 0</pose>
        <geometry>
          <mesh>
            <uri>model://team2_map/map.stl</uri>
            <!-- <scale>0.8825 0.8825 1</scale> -->
          </mesh>
        </geometry>
      </visual>
    </link>
    <static>1</static>
  </model>

  </world>
